{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "remix dev",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "format": "prettier --check ./app",
    "fix": "eslint --cache --cache-location ./node_modules/.cache/eslint . && prettier --write ./app",
    "typecheck": "tsc -b",
    "typecheck_cypress": "tsc -b cypress"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --cache-location ./node_modules/.cache/eslint . && prettier --write ./app",
      "tsc -b",
      "vitest run"
    ]
  },
  "dependencies": {
    "@braintree/sanitize-url": "^6.0.0",
    "@emotion/react": "^11.10.0",
    "@emotion/server": "^11.10.0",
    "@mantine/core": "^5.0.2",
    "@mantine/hooks": "^5.0.2",
    "@mantine/ssr": "^5.0.2",
    "@remix-run/node": "^1.6.7",
    "@remix-run/react": "^1.6.7",
    "@remix-run/vercel": "^1.6.7",
    "@supabase/supabase-js": "^1.35.4",
    "@vercel/node": "^2.5.3",
    "date-fns": "^2.29.1",
    "date-fns-tz": "^1.3.6",
    "fp-ts": "^2.12.2",
    "io-ts": "^2.2.17",
    "io-ts-types": "^0.5.16",
    "isbot": "^3.5.0",
    "marked": "^4.0.18",
    "monocle-ts": "^2.3.13",
    "newtype-ts": "^0.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "remix-auth": "^3.2.2",
    "remix-auth-supabase": "^4.0.0",
    "remix-validated-form": "^4.5.5",
    "sanitize-html": "^2.7.1",
    "tiny-invariant": "^1.2.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@relmify/jest-fp-ts": "^2.0.2",
    "@remix-run/dev": "^1.6.7",
    "@remix-run/eslint-config": "^1.6.7",
    "@remix-run/serve": "^1.6.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/marked": "^4.0.3",
    "@types/node": "^18.6.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/sanitize-html": "^2.6.2",
    "@types/validator": "^13.7.4",
    "@vitejs/plugin-react": "^2.0.0",
    "c8": "^7.12.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "happy-dom": "^6.0.4",
    "hasky": "^3.0.2",
    "jest-chain": "^1.1.5",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "scaffdog": "^1.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.21.1"
  },
  "engines": {
    "node": ">=16"
  },
  "prisma": {
